SELECT [ProductID]      ,isnull([Quantity],0) as[Quantity]   
  FROM [SalesDB].[dbo].[PurchaseDiscardsDetails]

)as TotalOutPutProducts
group by [ProductID] 
)as TotalOutPutProducts

on Products.ProductID=TotalOutPutProducts.ProductID</CommandText>
