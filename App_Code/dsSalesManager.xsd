from (
  SELECT [ProductID] ,isnull([Quantity],0) as[Quantity] 
  FROM [SalesDB].[dbo].[PurchasesDetails]
 
union All
SELECT [ProductID]      ,isnull([Quantity],0) as[Quantity]  
  FROM [SalesDiscardsDetails]

)as TotalInputProducts
group by [ProductID]  
)TotalInputProducts
