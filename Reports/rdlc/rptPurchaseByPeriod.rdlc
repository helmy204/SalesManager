      <Query>
        <DataSourceName>SalesDBConnectionString</DataSourceName>
        <CommandText>SELECT     dbo.PurchasesDetails.PurchasesDetailID, dbo.PurchasesDetails.PurchaseID,Convert(nvarchar(50), dbo.PurchasesMaster.PurchaseDateM,101) as PurchaseDateM, dbo.PurchasesMaster.PurchaseDateH, ISCash,Notes,
                      dbo.PurchasesMaster.SupplierID, dbo.Suppliers.SupplierName, dbo.PurchasesMaster.EmployeeID, dbo.Employees.EmployeeName, dbo.PurchasesDetails.ProductID, 
                      dbo.Products.ProductName, dbo.Products.CategoryID, dbo.Categories.CategoryName, dbo.PurchasesDetails.Quantity, dbo.PurchasesDetails.Price
FROM         dbo.PurchasesDetails INNER JOIN
                      dbo.PurchasesMaster ON dbo.PurchasesDetails.PurchaseID = dbo.PurchasesMaster.PurchaseID INNER JOIN
                      dbo.Suppliers ON dbo.PurchasesMaster.SupplierID = dbo.Suppliers.SupplierID INNER JOIN
                      dbo.Products ON dbo.PurchasesDetails.ProductID = dbo.Products.ProductID LEFT OUTER JOIN
                      dbo.Categories ON dbo.Products.CategoryID = dbo.Categories.CategoryID LEFT OUTER JOIN
                      dbo.Employees ON dbo.PurchasesMaster.EmployeeID = dbo.Employees.EmployeeID
where dbo.PurchasesMaster.PurchaseDateM &gt;=@StartDate and  dbo.PurchasesMaster.PurchaseDateM &lt;=@EndDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
